{
    "compilerOptions": {
      "module": "amd",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noImplicitReturns": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "target": "es5",
      "experimentalDecorators": true,
      "preserveConstEnums": true,
      "suppressImplicitAnyIndexErrors": true
    },
    "include": [
      "./app/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }
